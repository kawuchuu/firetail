{
    "settingsMenu": [{
        "info": {
            "title": "Settings"
        },
        "controls": [{
                "subButton": {
                    "title": "General",
                    "id": "generalSubButton",
                    "icon": "settings"
                }
            },
            {
                "subButton": {
                    "title": "Appearance",
                    "id": "appearanceSubButton",
                    "icon": "palette"
                }
            },
            {
                "subButton": {
                    "title": "About",
                    "id": "aboutSubButton",
                    "icon": "info"
                }
            }
        ]
    }],
    "generalSettings": [{
        "info": {
            "title": "General"
        },
        "controls": [{
                "switch": {
                    "title": "Discord Rich Presence",
                    "desc": "Sets currently playing song as your Discord status",
                    "id": "discordToggle"
                }
            },
            {
                "button": {
                    "title": "Reset Library",
                    "desc": "Resets your library back to first install.",
                    "id": "resetLibrary"
                }
            }
        ]
    }],
    "appearanceSettings": [{
        "info": {
            "title": "Appearance"
        },
        "controls": [{
                "select": {
                    "title": "Theme",
                    "desc": "Sets app colour theme to your chosen theme.",
                    "id": "themeSelect",
                    "options": [{
                            "option": {
                                "title": "Dark",
                                "value": "dark"
                            }
                        },
                        {
                            "option": {
                                "title": "Light",
                                "value": "light"
                            }
                        }
                    ]
                }
            },
            {
                "select": {
                    "title": "Colour Accent",
                    "desc": "Sets colour accent (eg. button colour, hover colour) to your chosen colour.",
                    "id": "colouraccentSelect",
                    "options": [{
                            "option": {
                                "title": "Firetail",
                                "value": "firetail"
                            }
                        },
                        {
                            "option": {
                                "title": "Audiation",
                                "value": "audiation"
                            }
                        },
                        {
                            "option": {
                                "title": "Nature",
                                "value": "nature"
                            }
                        },
                        {
                            "option": {
                                "title": "Aqua",
                                "value": "aqua"
                            }
                        }
                    ]
                }
            },
            {
                "select": {
                    "title": "Icon Style",
                    "desc": "Sets icons (eg.play/pause, repeat icons) to your chosen style.",
                    "id": "iconStyleSelect",
                    "options": [{
                            "option": {
                                "title": "Filled",
                                "value": "filled"
                            }
                        },
                        {
                            "option": {
                                "title": "Rounded",
                                "value": "rounded"
                            }
                        },
                        {
                            "option": {
                                "title": "Outlined",
                                "value": "outlined"
                            }
                        }
                    ]
                }
            },
            {
                "switch": {
                    "title": "Mini Player Background",
                    "desc": "Sets mini player's background to the song's album art",
                    "id": "miniBgToggle",
                    "toggled": true
                }
            }
        ]
    }],
    "aboutSettings": [{
        "info": {
            "title": "About"
        },
        "controls": [{
            "customHTML": ""
        },
        {
            "subButton": {
                "title": "Changelog",
                "id": "changelogSubButton",
                "icon": "notes"
            }
        },
        {
            "subButton": {
                "title": "Open Source Software",
                "id": "opensourceSubButton",
                "icon": "code"
            }
        }
    ]
    }],
    "changelogSettings": [{
        "info": {
            "title": "Changelog"
        },
        "controls": [{
            "customHTML": "<div id=\"changelog\"></div>"
        }]
    }],
    "opensourceSettings": [{
        "info": {
            "title": "Open Source Software"
        },
        "controls": [{
            "customHTML": "<div id=\"ossoftware\"></div>"
        }]
    }]
}